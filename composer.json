{
    "name": "jolimardi/laravel-honeypot",
    "description": "Laravel form spam protection with honeypot technique",
    "keywords": [
        "laravel",
        "honeypot",
        "form request",
        "laravel-honeypot",
        "validation"
    ],
    "homepage": "https://github.com/jolimardi/laravel-honeypot",
    "license": "MIT",
    "authors": [
        {
            "name": "WebLogin",
            "email": "contact@weblogin.fr"
        },
        {
            "name": "Romain Cherot",
            "email": "contact@cherot.fr"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/view": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "WebLogin\\LaravelHoneypot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebLogin\\LaravelHoneypot\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebLogin\\LaravelHoneypot\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
